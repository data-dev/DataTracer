{
    "name": "datatracer.foreign_key_composite.BasicCompositeForeignKeySolver",
    "description": "Detect the foreign keys of a Table.",
    "primitive": "datatracer.foreign_key_composite.BasicCompositeForeignKeySolver",
    "fit": {
        "method": "fit",
        "args": [
            {
                "name": "dict_of_databases",
                "type": "list"
            }
        ]
    },
    "produce": {
        "method": "solve",
        "args": [
            {
                "name": "tables",
                "type": "dict"
            },
            {
                "name": "primary_keys",
                "default": null,
                "type": "dict"
            }
        ],
        "output": [
            {
                "name": "foreign_keys",
                "type": "list"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "add_details": {
                "type": "bool",
                "description": "whether to add the foreign details such as the score obtained",
                "default": false
            }
        },
        "tunable": {
            "threshold": {
                "type": "float",
                "default": 0.9,
                "range": [
                    0.0,
                    1.0
                ]
            }
        }
    }
}

